<div class="login-as-employee">
    <h5>Login As Employee</h5>
    @Html.BeginForm("EmployeeHome", "Employee", FormMethod.Post, new { obj = Model })
 
    <form>
        <label>Login Id:</label>
        <input type="text" placeholder="Enter your Login Id" id="userId" />
        <input type="password" placeholder="Password" id = "password"/>
        <input type="submit" />
    </form>
  
</div>

